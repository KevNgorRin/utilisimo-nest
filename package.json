{
    "name": "@utilisimo/nest",
    "version": "0.0.2",
    "description": "NestJS library providing essential utilities and helpers for streamlined application setup and configuration.",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "nestjs",
        "utils",
        "utility",
        "helpers",
        "setup",
        "configuration"
    ],
    "author": {
        "name": "KevNgorRin",
        "email": "kevydev@outlook.com",
        "url": "https://github.com/KevNgorRin"
    },
    "homepage": "https://github.com/KevNgorRin/utilisimo-nest#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/KevNgorRin/utilisimo-nest.git"
    },
    "license": "MIT",
    "dependencies": {
        "@nestjs/common": "^11.0.0",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.0.0",
        "@nestjs/swagger": "^11.1.0",
        "@nestjs/typeorm": "^11.0.0",
        "class-validator": "^0.14.3",
        "typeorm": "^0.3.27"
    },
    "devDependencies": {
        "@nestjs/cli": "^11.0.5",
        "@nestjs/schematics": "^11.0.2",
        "@types/express": "^5.0.0",
        "@types/node": "^22.13.14",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^9.23.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.0.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.16",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
